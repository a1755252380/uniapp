<view class="main"><view class="accountBox"><input class="account" type="text" placeholder="用户昵称" focus="{{true}}" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['info']]]]]}}" value="{{info.name}}" bindinput="__e"/><input class="account" type="text" placeholder="请输入QQ号码作为登陆账号" data-event-opts="{{[['input',[['__set_model',['$0','account_num','$event',[]],['info']]]]]}}" value="{{info.account_num}}" bindinput="__e"/><input class="account" type="number" maxlength="13" placeholder="请输入手机号码用于找回密码" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['info']]]]]}}" value="{{info.phone}}" bindinput="__e"/><input class="pwd" type="text" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['info']]]]]}}" value="{{info.password}}" bindinput="__e"/><input class="pwd" type="text" placeholder="请再次输入密码" data-event-opts="{{[['blur',[['proofread',['$event']]]],['input',[['__set_model',['','pwd','$event',[]]]]]]}}" value="{{pwd}}" bindblur="__e" bindinput="__e"/><input class="account" type="text" placeholder="邀请码(非必填)" data-event-opts="{{[['input',[['__set_model',['$0','invite_code','$event',[]],['info']]]]]}}" value="{{info.invite_code}}" bindinput="__e"/></view><button class="btn" type="default" data-event-opts="{{[['tap',[['registered',['$event']]]]]}}" bindtap="__e">注册</button></view>