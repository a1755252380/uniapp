<view class="main"><view class="img"></view><view class="accountBox"><input class="account" type="text" placeholder="请输入账号" focus="{{true}}" data-event-opts="{{[['input',[['__set_model',['$0','account_num','$event',[]],['info']]]]]}}" value="{{info.account_num}}" bindinput="__e"/><input class="pwd" type="text" password="{{true}}" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['info']]]]]}}" value="{{info.password}}" bindinput="__e"/></view><view class="actionPwd"><view data-event-opts="{{[['tap',[['isCheck',['$event']]]]]}}" bindtap="__e"><label class="radio"><radio checked="{{ischeck}}"></radio><text>记住密码</text></label></view><view data-event-opts="{{[['tap',[['gotoForget',['$event']]]]]}}" bindtap="__e">忘记密码</view></view><view class="btn"><button data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">登陆</button><button data-event-opts="{{[['tap',[['gotoRegister',['$event']]]]]}}" bindtap="__e">注册</button></view></view>