<view class="main"><view class="img"></view><view class="accountBox"><input class="account" type="text" placeholder="请输入账号" focus="{{true}}"/><input class="pwd" type="text" password="{{true}}" placeholder="请输入密码"/></view><view class="actionPwd"><view><label class="radio"><radio value="" checked="{{ischeck}}" data-event-opts="{{[['tap',[['isCheck',['$event']]]]]}}" bindtap="__e"></radio><text>记住密码</text></label></view><view data-event-opts="{{[['tap',[['gotoForget',['$event']]]]]}}" bindtap="__e">忘记密码</view></view><view class="btn"><button type="default" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">登陆</button><button type="default" data-event-opts="{{[['tap',[['gotoRegister',['$event']]]]]}}" bindtap="__e">注册</button></view></view>